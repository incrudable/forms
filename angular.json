{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "forms": {
      "root": "libs/forms",
      "sourceRoot": "libs/forms/src",
      "projectType": "library",
      "prefix": "incrudable",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/forms/tsconfig.lib.json",
            "project": "libs/forms/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/forms/src/test.ts",
            "tsConfig": "libs/forms/tsconfig.spec.json",
            "karmaConfig": "libs/forms/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/forms/tsconfig.lib.json",
              "libs/forms/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      },
      "schematics": {}
    },
    "material-deps": {
      "root": "libs/material-deps",
      "sourceRoot": "libs/material-deps/src",
      "projectType": "library",
      "prefix": "mat-deps",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/material-deps/tsconfig.lib.json",
            "project": "libs/material-deps/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/material-deps/src/test.ts",
            "tsConfig": "libs/material-deps/tsconfig.spec.json",
            "karmaConfig": "libs/material-deps/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/material-deps/tsconfig.lib.json",
              "libs/material-deps/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      },
      "schematics": {}
    },
    "material-form-renderer": {
      "projectType": "library",
      "root": "libs/renderers/material-renderer",
      "sourceRoot": "libs/renderers/material-renderer/src",
      "prefix": "incrudable",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/renderers/material-renderer/tsconfig.lib.json",
            "project": "libs/renderers/material-renderer/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/renderers/material-renderer/tsconfig.lib.json",
              "libs/renderers/material-renderer/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/renderers/material-renderer/src/test.ts",
            "tsConfig": "libs/renderers/material-renderer/tsconfig.spec.json",
            "karmaConfig": "libs/renderers/material-renderer/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "demo": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/demo",
      "sourceRoot": "apps/demo/src",
      "prefix": "incrudable",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/demo",
            "index": "apps/demo/src/index.html",
            "main": "apps/demo/src/main.ts",
            "polyfills": "apps/demo/src/polyfills.ts",
            "tsConfig": "apps/demo/tsconfig.app.json",
            "aot": false,
            "assets": ["apps/demo/src/favicon.ico", "apps/demo/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/highlight.js/styles/default.css",
              "apps/demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo/src/environments/environment.ts",
                  "with": "apps/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build",
            "proxyConfig": "apps/demo/src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/demo/tsconfig.app.json",
              "apps/demo/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/demo/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/demo/jest.config.js",
            "tsConfig": "apps/demo/tsconfig.spec.json",
            "setupFile": "apps/demo/src/test-setup.ts"
          }
        }
      }
    },
    "demo-e2e": {
      "root": "apps/demo-e2e",
      "sourceRoot": "apps/demo-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo-e2e/cypress.json",
            "tsConfig": "apps/demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/demo-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/demo-e2e/**"]
          }
        }
      }
    },
    "ng-bootstrap-renderer": {
      "projectType": "library",
      "root": "libs/renderers/ng-bootstrap-renderer",
      "sourceRoot": "libs/renderers/ng-bootstrap-renderer/src",
      "prefix": "incrudable",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/renderers/ng-bootstrap-renderer/tsconfig.lib.json",
              "libs/renderers/ng-bootstrap-renderer/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/renderers/ng-bootstrap-renderer/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/renderers/ng-bootstrap-renderer/jest.config.js",
            "tsConfig": "libs/renderers/ng-bootstrap-renderer/tsconfig.spec.json",
            "setupFile": "libs/renderers/ng-bootstrap-renderer/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "ng-bootstrap-deps": {
      "projectType": "library",
      "root": "libs/ng-bootstrap-deps",
      "sourceRoot": "libs/ng-bootstrap-deps/src",
      "prefix": "incrudable",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ng-bootstrap-deps/tsconfig.lib.json",
              "libs/ng-bootstrap-deps/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/ng-bootstrap-deps/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ng-bootstrap-deps/jest.config.js",
            "tsConfig": "libs/ng-bootstrap-deps/tsconfig.spec.json",
            "setupFile": "libs/ng-bootstrap-deps/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "demo-ng-bootstrap": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/demo-ng-bootstrap",
      "sourceRoot": "apps/demo-ng-bootstrap/src",
      "prefix": "incrudable",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/demo-ng-bootstrap",
            "index": "apps/demo-ng-bootstrap/src/index.html",
            "main": "apps/demo-ng-bootstrap/src/main.ts",
            "polyfills": "apps/demo-ng-bootstrap/src/polyfills.ts",
            "tsConfig": "apps/demo-ng-bootstrap/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/demo-ng-bootstrap/src/favicon.ico",
              "apps/demo-ng-bootstrap/src/assets"
            ],
            "styles": ["apps/demo-ng-bootstrap/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo-ng-bootstrap/src/environments/environment.ts",
                  "with": "apps/demo-ng-bootstrap/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-ng-bootstrap:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-ng-bootstrap:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-ng-bootstrap:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/demo-ng-bootstrap/tsconfig.app.json",
              "apps/demo-ng-bootstrap/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/demo-ng-bootstrap/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/demo-ng-bootstrap/jest.config.js",
            "tsConfig": "apps/demo-ng-bootstrap/tsconfig.spec.json",
            "setupFile": "apps/demo-ng-bootstrap/src/test-setup.ts"
          }
        }
      }
    },
    "demo-ng-bootstrap-e2e": {
      "root": "apps/demo-ng-bootstrap-e2e",
      "sourceRoot": "apps/demo-ng-bootstrap-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo-ng-bootstrap-e2e/cypress.json",
            "tsConfig": "apps/demo-ng-bootstrap-e2e/tsconfig.e2e.json",
            "devServerTarget": "demo-ng-bootstrap:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo-ng-bootstrap:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/demo-ng-bootstrap-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/demo-ng-bootstrap-e2e/**"]
          }
        }
      }
    }
  },
  "defaultProject": "demo",
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/schematics:node-application": {
      "framework": "express"
    }
  }
}
